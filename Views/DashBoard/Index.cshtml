<!DOCTYPE html>
<html>
<head>
    <title>DashBoard</title>
    <link rel="stylesheet" href="~/css/dashboard.css" />
</head>
<body>
    <div id="container">
        @model User
        <h1 class="h1">Login feito com sucesso</h1>
        <h3>Client info</h3>
        <p>@Model.Email</p>
        <p>@Model.Senha</p>
        <p>@Model.Address.Cep</p>
        <p>@Model.Address.Localidade</p>
        <p>@Model.Address.UF</p>
        <p>@Model.Address.Logradouro</p>
        <p>@Model.Address.Bairro</p>
        <p>@Model.Address.Complemento</p>
    </div>
</body>
</html>
